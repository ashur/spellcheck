{
  "name": "spellcheck",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "eleventy --config=./src/_eleventy/config/.eleventy-site.js && npm run build:js",
    "build:bookmarklet": "eleventy --config=./src/_eleventy/config/.eleventy-bookmarklet.js",
    "build:js": "./scripts/build-js.sh",
    "dev": "concurrently \"npm:watch:*\"",
    "lint:bookmarklet": "eslint -c ./src/bookmarklet/.eslintrc.js ./build/*.js",
    "prebuild:js": "npm run lint:bookmarklet",
    "prelint:bookmarklet": "npm run build:bookmarklet",
    "serve": "eleventy --config=./src/_eleventy/config/.eleventy-site.js --serve",
    "test:integration": "cypress open",
    "test:unit": "mocha test",
    "watch:bookmarklet": "onchange -i './src/bookmarklet/**/*' -- npm run build:js",
    "watch:js": "DEBUG=true onchange -i 'src/scripts/*.*' -- npm run build:js",
    "watch:test": "onchange -i 'src/scripts/*.mjs' 'test/**/*.mjs' -- npm run test:unit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@squoosh/cli": "^0.7.2",
    "chai": "^4.3.4",
    "concurrently": "^6.5.1",
    "cypress": "^9.2.0",
    "eslint": "^8.13.0",
    "mocha": "^9.1.3",
    "netlify-cli": "^8.4.2",
    "onchange": "^7.1.0"
  },
  "dependencies": {
    "@11ty/eleventy": "^1.0.0",
    "clean-css": "^5.2.2",
    "esbuild": "^0.14.8",
    "html-minifier": "^4.0.0",
    "rollup": "^2.62.0"
  }
}
