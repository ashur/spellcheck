{
  "name": "spellcheck",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "eleventy && npm run build:js",
    "build:js": "esbuild src/scripts/bookmarklet.mjs --bundle --outfile=dist/bookmarklet.js",
    "dev": "concurrently \"npm:watch:*\"",
    "serve": "eleventy --serve",
    "test": "mocha test",
    "watch:js": "onchange -i 'src/scripts/*.mjs' -- npm run build:js",
    "watch:test": "onchange -i 'src/scripts/*.mjs' 'test/**/*.mjs' -- npm run test"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "chai": "^4.3.4",
    "concurrently": "^6.5.1",
    "eslint": "^8.5.0",
    "mocha": "^9.1.3",
    "netlify-cli": "^8.4.2",
    "onchange": "^7.1.0"
  },
  "dependencies": {
    "@11ty/eleventy": "^0.12.1",
    "esbuild": "^0.14.8",
    "rollup": "^2.62.0"
  }
}
